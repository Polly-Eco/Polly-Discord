{
   "name": "polly-pattern",
   "version": "1.0.0",
   "description": "Discord bot of Polly Ecosystem",
   "main": "./dist/src/index.js",
   "scripts": {
      "start": "ts-node src/index.ts",
      "start:dev": "nodemon",
      "build": "bash ./scripts/build.sh",
      "compile": "tsc --project tsconfig.build.json",
      "test": "jest",
      "lint": "eslint . --ext .ts",
      "lint:fix": "eslint . --ext .ts --fix",
      "prettier": "npx prettier --write ./src",
      "typeorm": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm",
      "migration:create": "bash ./scripts/createMigration.sh",
      "migration:up": "bash ./scripts/runMigration.sh",
      "migration:down": "bash ./scripts/revertMigration.sh"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/Polly-Eco/.git"
   },
   "keywords": [
      "repo",
      "pattern",
      "polly-eco"
   ],
   "author": "MonMon201",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/Polly-Eco/Polly-Pattern/issues"
   },
   "homepage": "https://github.com/Polly-Eco/Polly-Pattern#readme",
   "devDependencies": {
      "@types/jest": "^28.1.2",
      "@types/node": "^16.11.10",
      "@typescript-eslint/eslint-plugin": "^5.28.0",
      "@typescript-eslint/parser": "^5.28.0",
      "dotenv": "^16.0.1",
      "eslint": "^8.18.0",
      "eslint-plugin-jest": "^26.5.3",
      "eslint-plugin-prettier": "^4.0.0",
      "jest": "^28.1.1",
      "nodemon": "^2.0.16",
      "prettier": "^2.7.1",
      "sequelize-cli": "^6.4.1",
      "ts-jest": "^28.0.5",
      "ts-node": "10.7.0",
      "tslib": "^2.4.0",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "@discordjs/rest": "^1.1.0",
      "discord.js": "^14.3.0",
      "pg": "^8.4.0",
      "pg-hstore": "^2.3.4",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.9",
      "undici": "^5.5.1"
   }
}